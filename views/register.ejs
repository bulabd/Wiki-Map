<!DOCTYPE html>
<html lang="en">

<head>
  <title>Register Page</title>

  <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
  <!-- <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" /> -->
  <!-- <link rel="stylesheet" href="/styles/main.css" type="text/css" /> -->
  <!-- <link rel="stylesheet" href="/styles/layout.css" type="text/css" /> -->
  <link rel="stylesheet" href="/styles/register.css" type="text/css" />
  <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
  <script type="text/javascript" src="/scripts/app.js"></script>
</head>

<body>
  <header>
    <nav class="navBar">
      <h1>WikiMaps</h1>
      <a href="/login">Login</a>
    </nav>
  </header>
  <main class="mainPage">
    <h3>Create Accout</h3>
    <form class="ommitButton" method="POST" action="/register">
      <div class="form-group">
        <label for="email">Email</label>
        <input class="box" type="email" name="email" placeholder="Enter email">
      </div>
      <div class="form-group">
        <label for="password">Password</label>
        <input class="box" type="password" name="password" placeholder="Password">
      </div>
      <div class="form-group">
        <label for="name">Name</label>
        <input class="box" type="text" name="name" placeholder="Name">
      </div>
      <button type="submit">Submit</button>
    </form>
  </main>
</body>

</html>
