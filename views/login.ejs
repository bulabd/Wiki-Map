<body>
  <header>
    <nav class="navBar">
      <h1>WikiMaps</h1>
      <a href="/register">Sign Up</a>
    </nav>
  </header>
  <main class="mainPage">
    <h3>Login</h3>
    <form class="ommitButton" method="POST" action="/login">
      <div class="form-group">
        <label for="email">Email</label>
        <input class="box" type="email" name="email" placeholder="Enter email">
      </div>
      <div class="form-group">
        <label for="password">Password</label>
        <input class="box" type="password" name="password" placeholder="Password">
      </div>
      <button type="submit">Login</button>
    </form>
  </main>
</body>
