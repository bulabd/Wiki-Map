<!-- //receives an array of  available maps -->

<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css" integrity="sha384-zCbKRCUGaJDkqS1kPbPd7TveP5iyJE0EjAuZQTgFLD2ylzuqKfdKlfG/eSrtxUkn" crossorigin="anonymous">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://kit.fontawesome.com/b383617d0a.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="/styles/mapView.css" type="text/css" />
    <title>Map List</title>
  </head>
  <body>
    <div><i class="fa-solid fa-signs-post"></i></div>
    <h2>Hey Username!</h2>
    <div>
      <h2>Here are your favourite maps:</h2>
    </div>
    <div>
      <h2>Here are the maps you have contributed to:</h2>
    </div>
    <div>
      <h2>All maps:</h2>
      <div>
        <% for(const map in maps) { %>
          <div>
            <h1><%= maps[map].title %></h1>
          </div>
        <% } %>
      </div>
    </div>
  </body>
</html>
